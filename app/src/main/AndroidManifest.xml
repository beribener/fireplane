<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.carrollu.beribener.fireplane" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bm_player"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartScreenActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameEngine"
            android:label="@string/app_name"
            >
        </activity>
        <activity
            android:name=".HighScoresActivity"
            android:label="@string/title_activity_high_scores"
            >
        </activity>
    </application>

</manifest>
